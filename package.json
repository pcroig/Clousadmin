{
  "name": "clousadmin",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbopack",
    "dev:debug": "next dev --webpack",
    "build": "NODE_OPTIONS=--max-old-space-size=4096 next build",
    "start": "next start",
    "lint": "eslint",
    "test": "vitest run",
    "test:watch": "vitest",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:legacy": "tsx tests/index.ts",
    "db:test:setup": "dotenv -e .env.test -- prisma db push --skip-generate",
    "seed": "tsx prisma/seed.ts",
    "seed:plantillas": "tsx prisma/seed-plantillas.ts",
    "diagnostico": "tsx scripts/diagnostico-prisma.ts",
